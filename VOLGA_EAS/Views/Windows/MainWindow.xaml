<Window x:Class="VOLGA_EAS.MainWindow"
        xmlns:materialdesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm="clr-namespace:VOLGA_EAS.ViewModels"
        xmlns:v="clr-namespace:VOLGA_EAS.Views.Windows.MainWindowViews"
        xmlns:cmd="clr-namespace:VOLGA_EAS.Commands"
        xmlns:local="clr-namespace:VOLGA_EAS" xmlns:base="clr-namespace:VOLGA_EAS.ViewModels.Base" d:DataContext="{d:DesignInstance Type=vm:MainWindowViewModel}"
        mc:Ignorable="d"
        Title="{Binding Title}"
        Height="450"
        Width="800"
        Style="{StaticResource WindowStyle}">
    <Grid>
        <ContentControl Content="{Binding CurrentViewModel}">
            <ContentControl.Resources>
                <DataTemplate DataType="{x:Type vm:LoginViewModel}">
                    <v:LoginView/>
                </DataTemplate>
                <DataTemplate DataType="{x:Type vm:RegistrationViewModel}">
                    <v:RegistrationView/>
                </DataTemplate>
            </ContentControl.Resources>
        </ContentControl>
        <Button Height="50">
        </Button>
    </Grid>
</Window>
